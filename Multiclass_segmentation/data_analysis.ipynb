    "# 데이터 로더를 구현하기\n",
    "class Dataset(torch.utils.data.Dataset):\n",
    "    def __init__(self, data_dir, transform=None, type=None):\n",
    "        self.data_dir = data_dir\n",
    "        self.transform = transform\n",
    "        self.type = type\n",
    "\n",
    "        folder_name = 'ADC'\n",
    "        ADC_train_length = int(len(ADC_data_list)*0.8)\n",
    "        HGD_train_length = int(len(HGD_data_list)*0.8)\n",
    "        LGD_train_length = int(len(LGD_data_list)*0.8)\n",
    "        NOR_train_length = int(len(NOR_data_list)*0.8)\n",
    "\n",
    "        ADC_data_list[:ADC_train_length]\n",
    "        HGD_data_list[:HGD_train_length]\n",
    "        LGD_data_list[:LGD_train_length]\n",
    "        NOR_data_list[:NOR_train_length]\n",
    "\n",
    "        lst_label = []\n",
    "        lst_input = []\n",
    "\n",
    "        target_input = []\n",
    "        target_label = []\n",
    "\n",
    "        # train mode\n",
    "        if type == 0:\n",
    "            target_input = (ADC_data_list[:ADC_train_length] + HGD_data_list[:HGD_train_length] \n",
    "                                + LGD_data_list[:LGD_train_length] + NOR_data_list[:NOR_train_length])\n",
    "            target_label = (ADC_labeled_list[:ADC_train_length] + HGD_labeled_list[:HGD_train_length]\n",
    "                                + LGD_labeled_list[:LGD_train_length] + NOR_labeled_list[:NOR_train_length])\n",
    "        # test mode\n",
    "        else:\n",
    "            target_input = (ADC_data_list[ADC_train_length:] + HGD_data_list[HGD_train_length:] \n",
    "                                + LGD_data_list[LGD_train_length:] + NOR_data_list[NOR_train_length:])\n",
    "            target_label = (ADC_labeled_list[ADC_train_length:] + HGD_labeled_list[HGD_train_length:]\n",
    "                                + LGD_labeled_list[LGD_train_length:] + NOR_labeled_list[NOR_train_length:])\n",
    "        \n",
    "        self.lst_label = target_label\n",
    "        self.lst_input = target_input\n",
    "\n",
    "    def __len__(self):\n",
    "        return len(self.lst_label)\n",
    "\n",
    "    def __getitem__(self, index):\n",
    "        datapath = ''\n",
    "        zeros = np.zeros((256,256,1))\n",
    "        if 'ADC' in self.lst_input[index]:\n",
    "            datapath = os.path.join(self.data_dir, 'ADC')\n",
    "            label = np.asarray(((Image.open(os.path.join(datapath, self.lst_label[index]))).resize((256,256))))\n",
    "            tmp = label\n",
    "            label = np.dstack([label, tmp])\n",
    "            label = np.dstack([label, tmp])\n",
    "\n",
    "        elif 'HGD' in self.lst_input[index]:\n",
    "            datapath = os.path.join(self.data_dir, 'HGD')\n",
    "            label = np.asarray(((Image.open(os.path.join(datapath, self.lst_label[index]))).resize((256,256))))\n",
    "            tmp = label\n",
    "            label = np.dstack([label, tmp])\n",
    "            label = np.dstack([label, zeros])\n",
    "\n",
    "        elif 'LGD' in self.lst_input[index]:\n",
    "            datapath = os.path.join(self.data_dir, 'LGD')\n",
    "            label = np.asarray(((Image.open(os.path.join(datapath, self.lst_label[index]))).resize((256,256))))\n",
    "            tmp_zeros = np.dstack([zeros, zeros])\n",
    "            label = np.dstack([label, tmp_zeros])\n",
    "\n",
    "        elif 'NOR' in self.lst_input[index]:\n",
    "            datapath = os.path.join(self.data_dir, 'NOR')\n",
    "            label = np.asarray(((Image.open(os.path.join(datapath, self.lst_label[index]))).resize((256,256))))\n",
    "            label = np.dstack([label, zeros])\n",
    "            label = np.dstack([label, zeros])\n",
    "        \n",
    "        input = np.asarray((Image.open(os.path.join(datapath, self.lst_input[index])).resize((256,256))))\n",
    "\n",
    "        # 정규화\n",
    "        label[label>150] = 255\n",
    "        label[label<=150] = 0\n",
    "        \n",
    "        label = label/255.0\n",
    "        input = input/255.0\n",
    "\n",
    "        # 이미지와 레이블의 차원 = 2일 경우(채널이 없을 경우, 흑백 이미지), 새로운 채널(축) 생성\n",
    "        if label.ndim == 2:\n",
    "            label = label[:, :, np.newaxis]\n",
    "        if input.ndim == 2:\n",
    "            input = input[:, :, np.newaxis]\n",
    "\n",
    "        data = {'input': input, 'label': label}\n",
    "\n",
    "        # transform이 정의되어 있다면 transform을 거친 데이터를 불러옴\n",
    "        if self.transform:\n",
    "            data = self.transform(data)\n",
    "\n",
    "        return data"
   ]
